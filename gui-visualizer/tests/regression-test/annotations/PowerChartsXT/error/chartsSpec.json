                "annotations": {
                    "origw": "400",
                    "origh": "300",
                    "autoscale": "1",
                    "showbelow": "0",
                    "groups": [
                        {
                            "items": [
                                {
                                    "type": "rectangle",
                                    "color": "#00ff00",
                                    "alpha": "70",
                                    "x": "$captionStartX",
                                    "y": "$captionStartY",
                                    "tox": "$captionEndX",
                                    "toy": "$captionEndY"
                                },
                                {
                                    "type": "image",
                                    "url": "http://static.fusioncharts.com/sampledata/images/Logo-HM-72x72.png",
                                    "x": "$chartstartx",
                                    "y": "$chartstarty",
                                    "xScale": "100",
                                    "yScale": "70"
                                },
                                {
                                    "type": "circle",
                                    "x": "$chartstartx+20",
                                    "y": "$chartendy-20",
                                    "radius": "15",
                                    "color": "#0000ff"
                                },
                                {
                                    "type": "arc",
                                    "radius": "20",
                                    "borderColor": "#FFFFFF",
                                    "innerRadius": "5",
                                    "color": "#990033",
                                    "alpha": "90",
                                    "x": "$chartstartx+80",
                                    "y": "$chartendy-20",
                                    "startangle": "0",
                                    "endangle": "180"
                                },
                                {
                                    "type": "path",
                                    "path": "M -10, -10, L -10, 10, L 10, 10, L 10, -10, L -10, -10",
                                    "x": "$canvasstartx+20",
                                    "y": "$canvasstarty+20",
                                    "color": "#660000"
                                },
                                {
                                    "type": "polygon",
                                    "sides": "5",
                                    "startangle": "270",
                                    "fillColor": "#6baa01",
                                    "x": "$legendendx+40",
                                    "y": "$legendendy-10",
                                    "radius": "18"
                                },
                                {
                                    "type": "rectangle",
                                    "color": "#66FFFF",
                                    "alpha": "70",
                                    "x": "$subcaptionStartX",
                                    "y": "$subcaptionStartY",
                                    "tox": "$subcaptionEndX",
                                    "toy": "$subcaptionEndY"
                                },
                                {
                                    "id": "high-line",
                                    "type": "line",
                                    "x": "$canvasStartX",
                                    "y": "$dataset.0.set.2.y",
                                    "tox": "$canvasEndX",
                                    "toy": "$dataset.0.set.2.y",
                                    "color": "#6baa01",
                                    "dashed": "1",
                                    "thickness": "1"
                                },
                                {
                                    "id": "label",
                                    "type": "text",
                                    "text": "Line",
                                    "fillcolor": "#6baa01",
                                    "rotate": "90",
                                    "x": "$canvasEndX - 90",
                                    "y": "$dataset.0.set.2.y - 10"
                                }
                            ]
                        }
                    ]
                }
            }
        },
        "refimage": "./tests/smoke/annotations/PowerChartsXT/error/ref-images/annotations-error_01-snapshot.png",
        "id": "annotations-error_01"
    },
