    "annotations-spline_01": {
        "description": "annotations",
        "options": {
            "type": "spline",
            "width": "500",
            "height": "350",
            "dataFormat": "json",
            "dataSource": {
                "chart": {
                    "caption": "Bakersfield Central -Total footfalls",
                    "subCaption": "Last week",
                    "xAxisName": "Day",
                    "yAxisName": "No. of Visitors",
                    "animation": "0",
                    "charttopmargin": "50"
                },
                "annotations": {
                    "groups": [
                        {
                            "items": [
                                {
                                    "id": "high-line",
                                    "type": "line",
                                    "x": "$canvasStartX",
                                    "y": "$dataset.0.set.2.y",
                                    "tox": "$canvasEndX",
                                    "toy": "$dataset.0.set.2.y",
                                    "color": "#6baa01",
                                    "dashed": "1",
                                    "thickness": "1"
                                },
                                {
                                    "id": "label",
                                    "type": "text",
                                    "text": "Highest footfall",
                                    "fillcolor": "#6baa01",
                                    "rotate": "90",
                                    "x": "$canvasStartx+85",
                                    "y": "$dataset.0.set.2.y - 8",
                                    "align": "right"
                                }
                            ]
                        },
                        {
                            "items": [
                                {
                                    "id": "zone",
                                    "type": "rectangle",
                                    "x": "$xaxis.label.5.x",
                                    "y": "$canvasEndY",
                                    "tox": "$xaxis.label.6.x",
                                    "toy": "$canvasStartY",
                                    "color": "#6baa01",
                                    "alpha": "20"
                                },
                                {
                                    "id": "label",
                                    "type": "text",
                                    "text": "Weekend",
                                    "color": "#000000",
                                    "fontSize": "12",
                                    "bold": "1",
                                    "rotateText": "1",
                                    "x": "$xaxis.label.6.x - 10",
                                    "y": "$canvasEndY - 35"
                                },
                                {
                                    "id": "low-line",
                                    "type": "line",
                                    "x": "$canvasStartX",
                                    "y": "$dataset.0.set.3.y",
                                    "tox": "$canvasEndX",
                                    "toy": "$dataset.0.set.3.y",
                                    "color": "#d35400",
                                    "dashed": "1",
                                    "thickness": "1"
                                },
                                {
                                    "id": "label",
                                    "type": "text",
                                    "text": "Lowest footfall",
                                    "fillcolor": "#d35400",
                                    "rotate": "90",
                                    "x": "$canvasStartX+10",
                                    "y": "$dataset.0.set.3.y + 7",
                                    "align": "left"
                                },
                                {
                                    "type": "rectangle",
                                    "color": "#ff0000",
                                    "alpha": "70",
                                    "x": "$captionStartX",
                                    "y": "$captionStartY",
                                    "tox": "$captionEndX",
                                    "toy": "$captionEndY"
                                },
                                {
                                    "type": "image",
                                    "url": "http://static.fusioncharts.com/sampledata/images/Logo-HM-72x72.png",
                                    "x": "$chartstartx",
                                    "y": "$chartstarty",
                                    "xScale": "100",
                                    "yScale": "100"
                                },
                                {
                                    "type": "circle",
                                    "x": "$chartstartx+20",
                                    "y": "$chartendy-20",
                                    "radius": "15",
                                    "color": "#0000ff"
                                },
                                {
                                    "type": "arc",
                                    "radius": "20",
                                    "borderColor": "#FFFFFF",
                                    "innerRadius": "10",
                                    "color": "#990033",
                                    "alpha": "90",
                                    "x": "$captionStartX+100",
                                    "y": "$captionStartY",
                                    "startangle": "20",
                                    "endangle": "160"
                                },
                                {
                                    "type": "path",
                                    "path": "M -10, -10, L -10, 10, L 10, 10, L 10, -10, L -10, -10",
                                    "x": "$chartcenterx",
                                    "y": "$chartcentery",
                                    "color": "#660000"
                                },
                                {
                                    "type": "polygon",
                                    "sides": "4",
                                    "startangle": "270",
                                    "fillColor": "#6baa01",
                                    "x": "$chartendx-50",
                                    "y": "$chartstarty+50",
                                    "radius": "23"
                                },
                                {
                                    "type": "rectangle",
                                    "color": "#66FFFF",
                                    "alpha": "70",
                                    "x": "$subcaptionStartX",
                                    "y": "$subcaptionStartY",
                                    "tox": "$subcaptionEndX",
                                    "toy": "$subcaptionEndY"
                                }
                            ]
                        }
                    ]
                },
                "data": [
                    {
                        "label": "Mon",
                        "value": "15123"
                    },
                    {
                        "label": "Tue",
                        "value": "14233"
                    },
                    {
                        "label": "Wed",
                        "value": "25507"
                    },
                    {
                        "label": "Thu",
                        "value": "9110"
                    },
                    {
                        "label": "Fri",
                        "value": "15529"
                    },
                    {
                        "label": "Sat",
                        "value": "20803"
                    },
                    {
                        "label": "Sun",
                        "value": "19202"
                    }
                ]
            }
        },
        "refimage": "./tests/smoke/annotations/PowerChartsXT/spline/ref-images/annotations-spline_01-snapshot.png",
        "id": "annotations-spline_01"
    },
