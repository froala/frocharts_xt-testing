                "annotations": {
                    "origw": "450",
                    "origh": "300",
                    "autoscale": "1",
                    "showBelow": "0",
                    "groups": [
                        {
                            "id": "arcs",
                            "items": [
                                {
                                    "type": "rectangle",
                                    "color": "#ff0000",
                                    "alpha": "70",
                                    "x": "$captionStartX",
                                    "y": "$captionStartY",
                                    "tox": "$captionEndX",
                                    "toy": "$captionEndY"
                                },
                                {
                                    "type": "rectangle",
                                    "color": "#66FFFF",
                                    "alpha": "70",
                                    "x": "$subcaptionStartX",
                                    "y": "$subcaptionStartY",
                                    "tox": "$subcaptionEndX",
                                    "toy": "$subcaptionEndY"
                                },
                                {
                                    "type": "image",
                                    "url": "http://static.fusioncharts.com/sampledata/images/Logo-HM-72x72.png",
                                    "x": "$chartstartx",
                                    "y": "$chartstarty",
                                    "xScale": "100",
                                    "yScale": "100"
                                },
                                {
                                    "type": "circle",
                                    "x": "$gaugeStartX+40",
                                    "y": "$gaugeStarty+20",
                                    "radius": "25",
                                    "color": "#0000ff"
                                },
                                {
                                    "type": "arc",
                                    "radius": "20",
                                    "borderColor": "#FFFFFF",
                                    "innerRadius": "10",
                                    "color": "#990033",
                                    "alpha": "90",
                                    "x": "$gaugeStartX+30",
                                    "y": "$gaugeendy-30",
                                    "startangle": "0",
                                    "endangle": "360"
                                },
                                {
                                    "type": "path",
                                    "path": "M -10, -10, L -10, 10, L 10, 10, L 10, -10, L -10, -10",
                                    "x": "$gaugeCenterX",
                                    "y": "$gaugeCentery",
                                    "color": "#000000"
                                },
                                {
                                    "type": "line",
                                    "x": "$gaugeStartX",
                                    "y": "$chartcenterx+40",
                                    "tox": "$gaugeendX",
                                    "toy": "$chartcenterx+40",
                                    "thickness": "3",
                                    "color": "#0000ff"
                                },
                                {
                                    "type": "polygon",
                                    "sides": "7",
                                    "startangle": "270",
                                    "fillColor": "#ff0000",
                                    "x": "$plotSemiWidth+230",
                                    "y": "$plotSemiWidth-40",
                                    "radius": "30"
                                },
                                {
                                    "id": "state-cs-text",
                                    "type": "Text",
                                    "color": "#ff0000",
                                    "label": "Legend",
                                    "fontSize": "12",
                                    "align": "center",
                                    "x": "$legendendx+40",
                                    "y": "$legendendy-10"
                                }
                            ]
                        }
                    ]
                }
            }
        },
        "refimage": "./tests/smoke/annotations/FusionWidgetsXT/other/ref-images/annotations-other_01-snapshot.png",
        "id": "annotations-other_01"
    },
