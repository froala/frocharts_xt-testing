    "FT_FTV3-949_TC01": {
        "id": "FT_FTV3-949_TC01",
        "description": "SRS - Flat CSS style structure, introduced in V1.2 - FTV3-949",
        "options": {
            "data-id": "data-FT_TS14_TC01",
            "schema-id": "schema-FT_TS14_TC01",
            "type": "timeseries",
            "width": "900",
            "height": "500",
            "dataFormat": "json",
            "dataSource": {
                "extensions": {
                    "standardRangeSelector": {
                        "enabled": "1",
                        "style": {
                            "button-text": {
                                "fill": "#ffffff",
                                "font-family": "Source Sans Pro",
                                "font-size": "12px",
                                "line-height": "14px"
                            },
                            "button-background": {
                                "border-radius": "2px",
                                "fill": "#006600",
                                "fill-opacity": "0.5",
                                "_stroke-width": 1,
                                "stroke": "#000066",
                                "stroke-dasharray": "5 2"
                            },
                            "button-text:hover": {
                                "color": "#ffffff",
                                "font-family": "Source Sans Pro",
                                "font-size": "12px",
                                "font-weight": 600,
                                "line-height": "14px"
                            },
                            "button-background:hover": {
                                "border-radius": "2px",
                                "fill": "#333333",
                                "stroke-width": 1,
                                "stroke": "#ff0000",
                                "stroke-dasharray": "5 2"
                            },
                            "button-text:active": {
                                "color": "#006600",
                                "font-family": "Source Sans Pro",
                                "font-size": "12px",
                                "line-height": "14px"
                            },
                            "button-background:active": {
                                "border-radius": "2px",
                                "fill": "#ff0000",
                                "stroke-width": 1,
                                "stroke": "#00ffff",
                                "stroke-dasharray": "5 2",
                                "fill-opacity": "0.5"
                            },
                            "separator": {
                                "stroke": "#0000ff",
                                "stroke-opacity": "0.9",
                                "stroke-width": 3
                            }
                        }
                    }
                },
                "caption": {
                    "text": "Weather Forecast"
                },
                "subcaption": {
                    "text": "Visualization of different weather parameters"
                },
                "chart": {
                    "multiCanvas": 0,
                    "nimation": 0,
                    "animation": 0
                },
                "xAxis": [
                    {
                        "plot": "Date"
                    }
                ],
                "yAxis": [
                    {
                        "plot": [
                            {
                                "value": "Temp(°C)",
                                "type": "line"
                            }
                        ],
                        "format": {
                            "suffix": "°C",
                            "_prefix": "Temp "
                        },
                        "title": "Temperature  vs  Dew Point"
                    },
                    {
                        "plot": [
                            {
                                "value": "Wind Gust (kph)",
                                "type": "line"
                            }
                        ],
                        "format": {
                            "suffix": " kph",
                            "prefix": "Gust "
                        },
                        "title": "Wind Speed  vs  Wind Gust"
                    }
                ]
            }
        }
    },
