{
    "FTV3-1147-tc1": {
        "id": "FTV3-1147-tc1",
        "description": "Flag Data marker specified at a day level, shown for atleast month level bin",
        "options": {
            "data-id": "data-FTV3-1147",
            "schema-id": "schema-FTV3-1147",
            "type": "timeseries",
            "width": "800",
            "height": "600",
            "dataFormat": "json",
            "dataSource": {
                "yaxis": [
                    {
                        "plot": "Temperature",
                        "title": "Temperature",
                        "plotType": "line",
                        "format": {
                            "suffix": "°C",
                            "label": "Controlled Temperature",
                            "value": "10"
                        }
                    }
                ],
                "chart": {
                    "paletteColors": "#ff0000, #00ff00, #0000ff",
                    "animation": "0",
                    "multicanvas": 0
                },
                "xaxis": {
                    "binning": {
                        "month": [ 1 ],
                        "day": [],
                        "hour": [],
                        "minute": []
                    },
                    "initialInterval": {
                        "from": "01/01/2020 0:00:00",
                        "to": "07/31/2021 23:59:59"
                    }
                },
                "datamarker": [
                    {
                        "time": "05/25/2020",
                        "identifier": "A",
                        "timeformat": "%m/%d/%Y",
                        "tooltext": "Data marker",
                        "styleDefinition": {
                            "colorstyle": {
                                "fill": "#ff0000",
                                "font-weight": "bold"
                            }
                        }
                    }
                ],
                "caption": {
                    "text": "Temperature readings of an Italian Town"
                }
            }
        }
    },
